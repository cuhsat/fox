# Forensic Examiner plugins config
# Standard location is ~/.fx_plugins

[[Plugins.Start]]
# plugin name
Name = "Test Start"

# filename pattern
Path = "test"

# execute command with variables:
#
#   $+ path of open file
#   $. path of base file
#
Exec = "echo $. $+"

# plugin hotkey (F1-F8)
[Plugin.F1]

# plugin name
Name = "Test Plugin"

# execute command with variables:
#
#   $* path of open files
#   $+ path of open file
#   $. path of base file
#   $? user input value
#
Exec = "echo $?"

# requires user input
Input = "Test"
