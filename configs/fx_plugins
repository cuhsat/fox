# Forensic Examiner plugins config (~/.fx_plugins)

# available variables:
#
#  $FILE    path of open file
#  $BASE    path of base file
#
[[Plugins.Autostart]]

# plugin name
Name = "Test Autostart"

# filename pattern
Pattern = "test"

# execute command
Command = "echo $BASE $FILE"

# output type
Output = "text"

# available variables:
#
#  $INPUT   user input value
#  $FILES   path of open files
#  $FILE    path of open file
#  $BASE    path of base file
#
# available hotkeys:
#
#  F1 - F8
#
[Plugin.F1]

# plugin name
Name = "Test Plugin"

# user input prompt
Prompt = "Test"

# execute command
Command = "echo $BASE $FILE $FILES $INPUT"

# output type
Output = "text"
