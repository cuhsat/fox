# Forensic Examiner plugins config (~/.fox_plugins)

# available variables:
#
#  $FILE    path of open file
#  $BASE    path of base file
#
[Autostart.Info]

# plugin name
Name = "target-info"

# filename pattern
Pattern = '.*\.(dd|img|raw|ad1|asdf|E0?|00?)'

# execute command
Command = "target-info $FILE"

# output type
Output = "text"

# available variables:
#
#  $INPUT   user input value
#  $FILES   path of open files
#  $FILE    path of open file
#  $BASE    path of base file
#
# available hotkeys:
#
#  F1 - F7
#
[Plugin.F1]

# plugin name
Name = "target-query"

# user input prompt
Prompt = "Query"

# execute command
Command = "target-query -j -f $INPUT $BASE"

# output type
Output = "jsonl"
