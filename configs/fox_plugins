# Available variables:
#
#  {{FILE}}     file path (derived)
#  {{BASE}}     file path (originally)
#  {{TEMP}}     new random temp folder
#
# Available variables (only hotkey):
#
#  {{VALUE}}    user prompt value
#
[autostart.info]

# Plugin name
name = "target-info"

# Filepath regex pattern
pattern = '.*\.(dd|img|raw|ad1|asdf|E0?|00?)'

# List of commands to execute
commands = [
  'target-info "{{FILE}}"',
]

# Available hotkeys:
#
#  F7 - F12
#
[hotkey.f7]

# Plugin name
name = "target-query"

# User input prompt
prompt = "Query"

# List of commands to execute
commands = [
  'target-query -j -f "{{VALUE}}" "{{BASE}}"',
]
