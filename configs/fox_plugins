#
# ~/.fox_plugins - Forensic Examiner plugins file
#
# Available hotkeys:
#
# F7 - F12
#
# Available variables:
#
# {{dir}}       new random folder
# {{file}}      path of open file
# {{base}}      path of base file
# {{value}}     user prompt value (only hotkey)
#
[Autostart.Info]

# Plugin name
Name = "target-info"

# Filepath pattern
Pattern = '.*\.(dd|img|raw|ad1|asdf|E0?|00?)'

# Execute commands
Commands = [
  'target-info "{{file}}"',
]

[Hotkey.F7]

# Plugin name
Name = "target-query"

# User input prompt
Prompt = "Query"

# Execute commands
Commands = [
  'target-query -j -f "{{value}}" "{{base}}"',
]
